{
    "data_url": "https://loteriascaixa-api.herokuapp.com/api/megasena",
    "data_file": null,
    "export_file": "historico_e_previsoes_megasena_v4.xlsx",
    "sequence_length": 20,
    "num_features_base": 60,
    "num_features_time": 60,
    "rolling_freq_windows": [10, 50, 100],
    "gru_units": 192,
    "use_attention": true,
    "attention_type": "keras_additive",
    "use_batch_norm": true,
    "dropout_rate": 0.4,
    "epochs": 250,
    "batch_size": 64,
    "optimizer_type": "AdamW",
    "initial_learning_rate": 0.001,
    "lr_decay_steps": 10000,
    "lr_end_factor": 0.01,
    "adamw_weight_decay": 0.0001,
    "test_size_ratio": 0.15,
    "validation_split_ratio": 0.15,
    "cache_duration_hours": 24,
    "cache_dir": "cache",
    "tensorboard_log_dir": "logs/fit/"
}